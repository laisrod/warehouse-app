<h1>Novo Fornecedor</h1>

<%= form_with(model: @supplier) do |f| %>
  <div>
    <%= f.label :corporate_name %>
    <%= f.text.field :corporate_name %>
  </div>
  <div>
    <%= f.label :brand_name %>
    <%= f.text.area :brand_name %>
  </div>
  <div>
    <%= f.label :full_address %>
    <%= f.text.field :full_address %>
  </div>
  <div>
  <%= f.label :registration_number %>
  <%= f.text.field :registration_number %>
  </div>
  <div>
    <%= f.label :city %>
    <%= f.text.field :city %>
  </div>
  <div>
    <%= f.label :state %>
    <%= f.text.field :state %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.text.field :email %>
  </div>

<% end %>